    "# Matplotlib animation (copy into a notebook cell)\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib import animation\n",
    "from IPython.display import HTML\n",
    "\n",
    "# Example fallback arrays (comment out if you have real arrays)\n",
    "# t = np.linspace(0, 2*np.pi, 400)\n",
    "# x_e = 1.0 * np.cos(t)\n",
    "# y_e = 1.0 * np.sin(t)\n",
    "# x_m = -0.5 * np.cos(t)\n",
    "# y_m = -0.5 * np.sin(t)\n",
    "\n",
    "# Ensure all arrays have same length\n",
    "n = len(x_e)\n",
    "assert all(len(arr) == n for arr in (y_e, x_m, y_m)), \"All position arrays must have same length\"\n",
    "\n",
    "fig, ax = plt.subplots(figsize=(6,6))\n",
    "ax.set_aspect('equal', 'box')\n",
    "margin = 0.2\n",
    "xmin, xmax = min(x_e.min(), x_m.min()), max(x_e.max(), x_m.max())\n",
    "ymin, ymax = min(y_e.min(), y_m.min()), max(y_e.max(), y_m.max())\n",
    "dx = (xmax - xmin) or 1.0\n",
    "dy = (ymax - ymin) or 1.0\n",
    "ax.set_xlim(xmin - dx*margin, xmax + dx*margin)\n",
    "ax.set_ylim(ymin - dy*margin, ymax + dy*margin)\n",
    "ax.set_title(\"Earth & Moon animation (Matplotlib)\")\n",
    "\n",
    "# points\n",
    "pe, = ax.plot([], [], 'o', color='blue', markersize=8, label='Earth')\n",
    "pm, = ax.plot([], [], 'o', color='gray', markersize=6, label='Moon')\n",
    "# optional trails\n",
    "trail_len = 50\n",
    "te, = ax.plot([], [], '-', color='blue', alpha=0.4)\n",
    "tm, = ax.plot([], [], '-', color='gray', alpha=0.4)\n",
    "ax.legend()\n",
    "\n",
    "def init():\n",
    "    pe.set_data([], [])\n",
    "    pm.set_data([], [])\n",
    "    te.set_data([], [])\n",
    "    tm.set_data([], [])\n",
    "    return pe, pm, te, tm\n",
    "\n",
    "def update(i):\n",
    "    pe.set_data(x_e[i], y_e[i])\n",
    "    pm.set_data(x_m[i], y_m[i])\n",
    "    start = max(0, i - trail_len)\n",
    "    te.set_data(x_e[start:i+1], y_e[start:i+1])\n",
    "    tm.set_data(x_m[start:i+1], y_m[start:i+1])\n",
    "    return pe, pm, te, tm\n",
    "\n",
    "anim = animation.FuncAnimation(fig, update, frames=n, init_func=init, blit=True, interval=50)\n",
    "plt.close(fig)  # prevent double display\n",
    "\n",
    "# Display inline as HTML5 video (works in many notebooks)\n",
    "HTML(anim.to_jshtml())\n",
    "\n",
    "# To save as mp4 (requires ffmpeg), uncomment and run:\n",
    "# anim.save('earth_moon.mp4', fps=20, dpi=200)"
   ]
