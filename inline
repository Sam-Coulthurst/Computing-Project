  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "306ec290",
   "metadata": {},
   "outputs": [],
   "source": [
    "def pos_earth_moon(time_step,circular=True):\n",
    "    \"\"\"\n",
    "    This function returns the position of the Earth-Moon about it's barycenter\n",
    "\n",
    "    Inputs: \n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs\n",
    "        pos - Array size (2,2,int(max_time/timestep))\n",
    "        e.g. pos[0][0] gives the x position of the Earth at each time step\n",
    "             pos[0][1] gives the y position of the Earth at each time step\n",
    "             pos[1][0] gives the x position of the Moon at each time step\n",
    "             pos[1][1] gives the y position of the Moon at each time step\n",
    "    \"\"\"\n",
    "    if circular:\n",
    "        T = 2*np.pi*np.sqrt(d**3/(G*(m_e + m_m)))\n",
    "        # Angular velocity\n",
    "        omega = 2*np.pi/T # rad/s\n",
    "\n",
    "        # Time array\n",
    "        max_time = 2*T\n",
    "        time = np.arange(0, max_time, time_step) #s\n",
    "\n",
    "        # Position arrays\n",
    "        pos = np.zeros((2,2,len(time)))\n",
    "\n",
    "        # Earth position (assuming circular orbit around barycenter)\n",
    "        r_e = d*m_m/(m_e + m_m) # m\n",
    "        x_e = r_e * np.cos(omega*time) # x position of Earth, m\n",
    "        y_e = r_e * np.sin(omega*time) # y position of Earth, m\n",
    "\n",
    "        # Moon position (assuming circular orbit around barycenter)\n",
    "        r_m = d*m_e/(m_e + m_m) # m\n",
    "        x_m = -r_m * np.cos(omega*time) # x position of Moon, m\n",
    "        y_m = -r_m * np.sin(omega*time) # y position of Moon, m\n",
    "        return x_e, y_e, x_m, y_m\n",
    "    else:\n",
    "        raise(NotImplementedError(\"Elliptical orbits not yet implemented\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "id": "14045437",
   "metadata": {},
   "outputs": [],
   "source": [
    "G = 6.67430e-11 # m^3 kg^-1 s^-2\n",
    "m_e = 5.972e24 # kg\n",
    "m_m = 7.348e22 # kg\n",
    "d = 384400000 # m\n",
    "time_step = 3600 #s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "id": "b8b5288b",
   "metadata": {},
   "outputs": [],
   "source": [
    "x_r0 = 7000000 # m\n",
    "y_r0 = 0 # m\n",
    "\n",
    "x_r_dot0 = 0 # m/s\n",
    "y_r_dot0 = 7500 # m/s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "id": "e04a0915",
   "metadata": {},
   "outputs": [],
   "source": [
    "r_rocket = [0, 6371000] # m\n",
    "\n",
    "v_rocket = [0, 7500] # m/s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "id": "c0d36d9a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ODE(x,y):\n",
    "    x_e, y_e, x_m, y_m = pos_earth_moon(time_step)\n",
    "    d_e = np.sqrt((x - x_e)**2 + (y - y_e)**2)\n",
    "    d_m = np.sqrt((x - x_m)**2 + (y - y_m)**2)\n",
    "    return -G*m_e*(x-x_e)/d_e**3 - G*m_m*(x-x_m)/d_m**3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "id": "d0e0572d",
   "metadata": {},
   "outputs": [],
   "source": [
    "def pos_earth_moon(time_step,circular=True):\n",
    "    \"\"\"\n",
    "    This function returns the position of the Earth-Moon about it's barycenter\n",
    "\n",
    "    Inputs: \n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs\n",
    "        pos - Array size (2,2,int(max_time/timestep))\n",
    "        e.g. pos[0][0] gives the x position of the Earth at each time step\n",
    "             pos[0][1] gives the y position of the Earth at each time step\n",
    "             pos[1][0] gives the x position of the Moon at each time step\n",
    "             pos[1][1] gives the y position of the Moon at each time step\n",
    "    \"\"\"\n",
    "    if circular:\n",
    "        # Angular velocity\n",
    "        omega = 2*np.pi/T # rad/s\n",
    "\n",
    "        # Position arrays\n",
    "        pos = np.zeros((2,2,len(time)))\n",
    "\n",
    "        # Earth position (assuming circular orbit around barycenter)\n",
    "        r_e = d*m_m/(m_e + m_m) # m\n",
    "        x_e = r_e * np.cos(omega*time) # x position of Earth, m\n",
    "        y_e = r_e * np.sin(omega*time) # y position of Earth, m\n",
    "\n",
    "        # Moon position (assuming circular orbit around barycenter)\n",
    "        r_m = d*m_e/(m_e + m_m) # m\n",
    "        x_m = -r_m * np.cos(omega*time) # x position of Moon, m\n",
    "        y_m = -r_m * np.sin(omega*time) # y position of Moon, m\n",
    "        return x_e, y_e, x_m, y_m\n",
    "    else:\n",
    "        raise(NotImplementedError(\"Elliptical orbits not yet implemented\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 50,
   "id": "d5b58af6",
   "metadata": {},
   "outputs": [],
   "source": [
    "T = 2*np.pi*np.sqrt(d**3/(G*(m_e + m_m)))\n",
    "max_time = 2*T\n",
    "time = np.arange(0, max_time, time_step) #s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "id": "535910fb",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Taylor expansion\n",
    "\n",
    "def taylor_expansion(ODE, time_step):\n",
    "    '''\n",
    "    This function solves a 2nd order ODE using a Taylor expansion with time step: time_step\n",
    "\n",
    "    Inputs:\n",
    "        ODE - function that gives equation of 2nd order ODE\n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs:\n",
    "        r_rocket - position of the rocket at each time step\n",
    "    '''\n",
    "    time = np.arange(0, max_time, time_step) #s\n",
    "    x_r = np.zeros(len(time_step))\n",
    "    x_r = r_rocket[0] + time_step*v_rocket[0] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1])\n",
    "    y_r = r_rocket[1] + time_step*v_rocket[1] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1])\n",
    "\n",
    "\n",
    "\n",
    "    return r_rocket"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 52,
   "id": "3415e244",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ODE(x,y,i):\n",
    "    x_e, y_e, x_m, y_m = pos_earth_moon(time_step)\n",
    "    d_e = np.sqrt((x - x_e[i])**2 + (y - y_e[i])**2)\n",
    "    d_m = np.sqrt((x - x_m[i])**2 + (y - y_m[i])**2)\n",
    "    return -G*m_e*(x-x_e)/d_e**3 - G*m_m*(x-x_m)/d_m**3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 53,
   "id": "ff02bea5",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ODE(x,y,i):\n",
    "    x_e, y_e, x_m, y_m = pos_earth_moon(time_step)\n",
    "    d_e = np.sqrt((x - x_e[i])**2 + (y - y_e[i])**2)\n",
    "    d_m = np.sqrt((x - x_m[i])**2 + (y - y_m[i])**2)\n",
    "    return -G*m_e*(x-x_e[i])/d_e**3 - G*m_m*(x-x_m[i])/d_m**3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 54,
   "id": "8fed6003",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Taylor expansion\n",
    "\n",
    "def taylor_expansion(ODE, time_step):\n",
    "    '''\n",
    "    This function solves a 2nd order ODE using a Taylor expansion with time step: time_step\n",
    "\n",
    "    Inputs:\n",
    "        ODE - function that gives equation of 2nd order ODE\n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs:\n",
    "        r_rocket - position of the rocket at each time step\n",
    "    '''\n",
    "    time = np.arange(0, max_time, time_step) #s\n",
    "    x_r = np.zeros(len(time))\n",
    "    y_r = np.zeros(len(time))\n",
    "\n",
    "    x_r[0] = r_rocket[0]\n",
    "    y_r[0] = r_rocket[1]\n",
    "\n",
    "    for i in range(1, len(time)):\n",
    "        x_r[i] = x_r[i-1] + time_step*v_rocket[0] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "        y_r[i] = y_r[i-1] + time_step*v_rocket[1] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "\n",
    "\n",
    "\n",
    "    return r_rocket"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 55,
   "id": "2e0ae775",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Taylor expansion\n",
    "\n",
    "def taylor_expansion(ODE, time_step):\n",
    "    '''\n",
    "    This function solves a 2nd order ODE using a Taylor expansion with time step: time_step\n",
    "\n",
    "    Inputs:\n",
    "        ODE - function that gives equation of 2nd order ODE\n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs:\n",
    "        r_rocket - position of the rocket at each time step\n",
    "    '''\n",
    "    time = np.arange(0, max_time, time_step) #s\n",
    "    x_r = np.zeros(len(time))\n",
    "    y_r = np.zeros(len(time))\n",
    "\n",
    "    x_r[0] = r_rocket[0]\n",
    "    y_r[0] = r_rocket[1]\n",
    "\n",
    "    for i in range(1, len(time)):\n",
    "        x_r[i] = x_r[i-1] + time_step*v_rocket[0] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "        y_r[i] = y_r[i-1] + time_step*v_rocket[1] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "\n",
    "    r_rocket = [x_r, y_r]\n",
    "\n",
    "    return r_rocket"
   ]
  },
