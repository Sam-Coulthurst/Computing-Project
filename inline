    "try:\n",
    "    # use Earth positions as an example; replace with any x(t), y(t)\n",
    "    display(animate_2d(x_e, y_e, title=\"Earth orbit (example)\"))\n",
    "except NameError:\n",
    "    print('x_e/y_e not found. Call pos_earth_moon(time_step) first or provide x,y arrays.')"
   ]
