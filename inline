  {
   "cell_type": "code",
   "execution_count": 92,
   "id": "0ec62c6e",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Taylor expansion\n",
    "\n",
    "def taylor_expansion(ODE, time_step,r_rocket):\n",
    "    '''\n",
    "    This function solves a 2nd order ODE using a Taylor expansion with time step: time_step\n",
    "\n",
    "    Inputs:\n",
    "        ODE - function that gives equation of 2nd order ODE\n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs:\n",
    "        r_rocket - position of the rocket at each time step\n",
    "    '''\n",
    "    x_r = np.zeros(len(time))\n",
    "    y_r = np.zeros(len(time))\n",
    "    x_r[0] = r_rocket[0]\n",
    "    y_r[0] = r_rocket[1]\n",
    "\n",
    "    vx_r = np.zeros(len(time))\n",
    "    vy_r = np.zeros(len(time))\n",
    "    vx_r[0] = v_rocket[0]\n",
    "    vy_r[0] = v_rocket[1]\n",
    "\n",
    "\n",
    "    for i in range(1, len(time)):\n",
    "        x_r[i] = x_r[i-1] + time_step*vx_r[i-1] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "        vx_r[i] = vx_r[i-1] + time_step*ODE(r_rocket[0], r_rocket[1],i) \n",
    "\n",
    "        y_r[i] = y_r[i-1] + time_step*vy_r[i-1] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "        vy_r[i] = vy_r[i-1] + time_step*ODE(r_rocket[0], r_rocket[1],i)\n",
    "\n",
    "    r_rocket = [x_r, y_r]\n",
    "\n",
    "    return x_r, y_r"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 93,
