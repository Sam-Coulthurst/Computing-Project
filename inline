   "cell_type": "code",
   "execution_count": 22,
   "id": "5d02d96e",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Simple reusable 2D animation helper\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib import animation\n",
    "from IPython.display import HTML\n",
    "import numpy as np\n",
    "\n",
    "def animate_2d(x, y, t=None, trail=30, interval=50, figsize=(6,6), title=\"2D animation\"):\n",
    "    \"\"\"\n",
    "    Animate 2D positions given arrays x(t), y(t) and optional time vector t.\n",
    "\n",
    "    Inputs:\n",
    "      x, y : 1D numpy arrays of same length (positions over time)\n",
    "      t     : optional time array (used only to set number of frames if provided)\n",
    "      trail : number of previous points to show as a trail (default 30)\n",
    "      interval : ms between frames (default 50)\n",
    "      figsize : figure size tuple\n",
    "      title : plot title string\n",
    "\n",
    "    Returns:\n",
    "      IPython.display.HTML object containing the animation (JS) for inline display\n",
    "    \"\"\"\n",
    "    x = np.asarray(x)\n",
    "    y = np.asarray(y)\n",
    "    if x.shape != y.shape:\n",
    "        raise ValueError('x and y must have the same shape')\n",
    "    n = x.size\n",
    "    frames = n if t is None else len(t)\n",
    "\n",
    "    fig, ax = plt.subplots(figsize=figsize)\n",
    "    ax.set_aspect('equal', 'box')\n",
    "    margin = 0.15\n",
    "    xmin, xmax = x.min(), x.max()\n",
    "    ymin, ymax = y.min(), y.max()\n",
    "    dx = (xmax - xmin) or 1.0\n",
    "    dy = (ymax - ymin) or 1.0\n",
    "    ax.set_xlim(xmin - dx*margin, xmax + dx*margin)\n",
    "    ax.set_ylim(ymin - dy*margin, ymax + dy*margin)\n",
    "    ax.set_title(title)\n",
    "\n",
    "    point, = ax.plot([], [], 'o', color='C0')\n",
    "    trail_line, = ax.plot([], [], '-', color='C0', alpha=0.4)\n",
    "\n",
    "    def init():\n",
    "        point.set_data([], [])\n",
    "        trail_line.set_data([], [])\n",
    "        return point, trail_line\n",
    "\n",
    "    def update(i):\n",
    "        # Ensure we pass sequences to set_data (Matplotlib requirement)\n",
    "        xi = x[i]\n",
    "        yi = y[i]\n",
    "        xi_arr = np.atleast_1d(xi).astype(float)\n",
    "        yi_arr = np.atleast_1d(yi).astype(float)\n",
    "        px = list(xi_arr)\n",
    "        py = list(yi_arr)\n",
    "\n",
    "        point.set_data(px, py)\n",
    "\n",
    "        start = max(0, i - trail)\n",
    "        tx = list(np.asarray(x[start:i+1]).astype(float))\n",
    "        ty = list(np.asarray(y[start:i+1]).astype(float))\n",
    "        trail_line.set_data(tx, ty)\n",
    "        return point, trail_line\n",
    "\n",
    "    anim = animation.FuncAnimation(fig, update, frames=frames, init_func=init,\n",
    "                                   blit=True, interval=interval)\n",
    "    plt.close(fig)\n",
    "    return HTML(anim.to_jshtml())"
   ]
  },
  {
