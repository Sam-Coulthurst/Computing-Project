    "def ODE(x,y):\n",
    "\n",
    "    pos_earth, pos_moon = pos_earth_moon(time_step)\n",
    "    d_e = np.sqrt((x - pos_earth[0])**2 + (y - pos_earth[1])**2)\n",
    "    d_m = np.sqrt((x - pos_moon[0])**2 + (y - pos_moon[1])**2)\n",
    "\n",
    "    a_rocket = [-G*m_e*(x-pos_earth[0])/d_e**3 - G*m_m*(x-pos_moon[0])/d_m**3,\n",
    "                -G*m_e*(y-pos_earth[1])/d_e**3 - G*m_m*(y-pos_moon[1])/d_m**3]\n",
    "\n",
    "    return a_rocket"
   ]
