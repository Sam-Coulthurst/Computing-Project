    "ax.set_ylabel('Y Position (m)')\n",
    "ax.set_title('Earth and Moon Orbits relative to Barycenter')\n",
    "ax.legend()\n",
    "ax.grid(True)\n",
    "#ax.set_xlim(-5e9, 5e9)\n",
    "#ax.set_ylim(-5e9, 5e9)\n",
