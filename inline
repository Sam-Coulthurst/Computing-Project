  {
   "cell_type": "code",
   "execution_count": 80,
   "id": "64e5d606",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 81,
   "id": "887b085c",
   "metadata": {},
   "outputs": [],
   "source": [
    "G = 6.67430e-11 # m^3 kg^-1 s^-2\n",
    "m_e = 5.972e24 # kg\n",
    "m_m = 7.348e22 # kg\n",
    "d = 384400000 # m\n",
    "time_step = 3600 #s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 82,
   "id": "1c9681ae",
   "metadata": {},
   "outputs": [],
   "source": [
    "r_rocket = [0, 6371000] # m\n",
    "\n",
    "v_rocket = [0, 750] # m/s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 83,
   "id": "5271fca7",
   "metadata": {},
   "outputs": [],
   "source": [
    "T = 2*np.pi*np.sqrt(d**3/(G*(m_e + m_m)))\n",
    "max_time = 2*T\n",
    "time = np.arange(0, max_time, time_step) #s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 84,
   "id": "677869a3",
   "metadata": {},
   "outputs": [],
   "source": [
    "def pos_earth_moon(time_step,circular=True):\n",
    "    \"\"\"\n",
    "    This function returns the position of the Earth-Moon about it's barycenter\n",
    "\n",
    "    Inputs: \n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs\n",
    "        pos - Array size (2,2,int(max_time/timestep))\n",
    "        e.g. pos[0][0] gives the x position of the Earth at each time step\n",
    "             pos[0][1] gives the y position of the Earth at each time step\n",
    "             pos[1][0] gives the x position of the Moon at each time step\n",
    "             pos[1][1] gives the y position of the Moon at each time step\n",
    "    \"\"\"\n",
    "    if circular:\n",
    "        # Angular velocity\n",
    "        omega = 2*np.pi/T # rad/s\n",
    "\n",
    "        # Position arrays\n",
    "        pos = np.zeros((2,2,len(time)))\n",
    "\n",
    "        # Earth position (assuming circular orbit around barycenter)\n",
    "        r_e = d*m_m/(m_e + m_m) # m\n",
    "        x_e = r_e * np.cos(omega*time) # x position of Earth, m\n",
    "        y_e = r_e * np.sin(omega*time) # y position of Earth, m\n",
    "\n",
    "        # Moon position (assuming circular orbit around barycenter)\n",
    "        r_m = d*m_e/(m_e + m_m) # m\n",
    "        x_m = -r_m * np.cos(omega*time) # x position of Moon, m\n",
    "        y_m = -r_m * np.sin(omega*time) # y position of Moon, m\n",
    "        return x_e, y_e, x_m, y_m\n",
    "    else:\n",
    "        raise(NotImplementedError(\"Elliptical orbits not yet implemented\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 85,
   "id": "7c847861",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Example usage\n",
    "x_e, y_e, x_m, y_m = pos_earth_moon(time_step)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 86,
   "id": "eaf57635",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create figure and axes explicitly\n",
    "fig = plt.figure(figsize=(8, 8))  # adjustable figure size\n",
    "ax = plt.axes()  # creates a single Axes object\n",
    "\n",
    "# Plot on the specific Axes\n",
    "ax.plot(x_e, y_e, label='Earth Orbit')\n",
    "ax.plot(x_m, y_m, label='Moon Orbit')\n",
    "\n",
    "# Add labels, legend, grid\n",
    "ax.set_xlabel('X Position (m)')\n",
    "ax.set_ylabel('Y Position (m)')\n",
    "ax.set_title('Earth and Moon Orbits relative to Barycenter')\n",
    "ax.legend()\n",
    "ax.grid(True)\n",
    "\n",
    "# Equal aspect ratio for orbits\n",
    "ax.set_aspect('equal', 'box')\n",
    "\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 87,
   "id": "2a5e93c6",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ODE(x,y,i):\n",
    "    x_e, y_e, x_m, y_m = pos_earth_moon(time_step)\n",
    "    d_e = np.sqrt((x - x_e[i])**2 + (y - y_e[i])**2)\n",
    "    d_m = np.sqrt((x - x_m[i])**2 + (y - y_m[i])**2)\n",
    "    return -G*m_e*(x-x_e[i])/d_e**3 - G*m_m*(x-x_m[i])/d_m**3"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 88,
   "id": "295feed6",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Taylor expansion\n",
    "\n",
    "def taylor_expansion(ODE, time_step,r_rocket):\n",
    "    '''\n",
    "    This function solves a 2nd order ODE using a Taylor expansion with time step: time_step\n",
    "\n",
    "    Inputs:\n",
    "        ODE - function that gives equation of 2nd order ODE\n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs:\n",
    "        r_rocket - position of the rocket at each time step\n",
    "    '''\n",
    "    x_r = np.zeros(len(time))\n",
    "    y_r = np.zeros(len(time))\n",
    "    x_r[0] = r_rocket[0]\n",
    "    y_r[0] = r_rocket[1]\n",
    "\n",
    "    for i in range(1, len(time)):\n",
    "        x_r[i] = x_r[i-1] + time_step*v_rocket[0] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "        y_r[i] = y_r[i-1] + time_step*v_rocket[1] + 0.5*time_step**2*ODE(r_rocket[0], r_rocket[1],i)\n",
    "\n",
    "    r_rocket = [x_r, y_r]\n",
    "\n",
    "    return x_r, y_r"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 89,
   "id": "e7a2039d",
   "metadata": {},
   "outputs": [],
   "source": [
    "x_r = np.zeros(len(time))\n",
    "x_r[0] = r_rocket[0]"
   ]
  },
