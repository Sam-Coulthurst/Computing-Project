  {
   "cell_type": "code",
   "execution_count": 25,
   "id": "d9c19853",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Simple reusable 2D animation helper supporting two trajectories\n",
    "import matplotlib.pyplot as plt\n",
    "from matplotlib import animation\n",
    "from IPython.display import HTML\n",
    "import numpy as np\n",
    "\n",
    "def animate_2d(x1, y1, x2=None, y2=None, t=None, trail=30, interval=50,\n",
    "               figsize=(6,6), title=\"2D animation\"):\n",
    "    \"\"\"\n",
    "    Animate one or two 2D trajectories given arrays x(t), y(t) and optional time vector t.\n",
    "\n",
    "    Inputs:\n",
    "      x1, y1 : 1D numpy arrays (first trajectory)\n",
    "      x2, y2 : optional 1D numpy arrays (second trajectory)\n",
    "      t      : optional time array (sets number of frames)\n",
    "      trail  : number of previous points to show as a trail (default 30)\n",
    "      interval : ms between frames (default 50)\n",
    "      figsize : figure size tuple\n",
    "      title   : plot title string\n",
    "\n",
    "    Returns:\n",
    "      IPython.display.HTML object containing the animation (JS) for inline display\n",
    "    \"\"\"\n",
    "    x1, y1 = np.asarray(x1), np.asarray(y1)\n",
    "    if x1.shape != y1.shape:\n",
    "        raise ValueError(\"x1 and y1 must have the same shape\")\n",
    "\n",
    "    if x2 is not None and y2 is not None:\n",
    "        x2, y2 = np.asarray(x2), np.asarray(y2)\n",
    "        if x2.shape != y2.shape:\n",
    "            raise ValueError(\"x2 and y2 must have the same shape\")\n",
    "        n = min(len(x1), len(x2))\n",
    "    else:\n",
    "        n = len(x1)\n",
    "\n",
    "    frames = n if t is None else len(t)\n",
    "\n",
    "    # Setup figure and axes\n",
    "    fig, ax = plt.subplots(figsize=figsize)\n",
    "    ax.set_aspect('equal', 'box')\n",
    "    margin = 0.15\n",
    "\n",
    "    all_x = x1 if x2 is None else np.concatenate([x1, x2])\n",
    "    all_y = y1 if y2 is None else np.concatenate([y1, y2])\n",
    "\n",
    "    xmin, xmax = all_x.min(), all_x.max()\n",
    "    ymin, ymax = all_y.min(), all_y.max()\n",
    "    dx = (xmax - xmin) or 1.0\n",
    "    dy = (ymax - ymin) or 1.0\n",
    "    ax.set_xlim(xmin - dx*margin, xmax + dx*margin)\n",
    "    ax.set_ylim(ymin - dy*margin, ymax + dy*margin)\n",
    "    ax.set_title(title)\n",
    "\n",
    "    # Primary object\n",
    "    point1, = ax.plot([], [], 'o', color='C0')\n",
    "    trail1, = ax.plot([], [], '-', color='C0', alpha=0.4)\n",
    "\n",
    "    # Optional secondary object\n",
    "    if x2 is not None and y2 is not None:\n",
    "        point2, = ax.plot([], [], 'o', color='C1')\n",
    "        trail2, = ax.plot([], [], '-', color='C1', alpha=0.4)\n",
    "    else:\n",
    "        point2 = trail2 = None\n",
    "\n",
    "    def init():\n",
    "        point1.set_data([], [])\n",
    "        trail1.set_data([], [])\n",
    "        if point2:\n",
    "            point2.set_data([], [])\n",
    "            trail2.set_data([], [])\n",
    "        return (point1, trail1) if point2 is None else (point1, trail1, point2, trail2)\n",
    "\n",
    "    def update(i):\n",
    "        # --- first trajectory\n",
    "        point1.set_data(x1[i], y1[i])\n",
    "        start1 = max(0, i - trail)\n",
    "        trail1.set_data(x1[start1:i+1], y1[start1:i+1])\n",
    "\n",
    "        artists = [point1, trail1]\n",
    "\n",
    "        # --- second trajectory (if present)\n",
    "        if point2:\n",
    "            point2.set_data(x2[i], y2[i])\n",
    "            start2 = max(0, i - trail)\n",
    "            trail2.set_data(x2[start2:i+1], y2[start2:i+1])\n",
    "            artists += [point2, trail2]\n",
    "\n",
    "        return artists\n",
    "\n",
    "    anim = animation.FuncAnimation(fig, update, frames=frames, init_func=init,\n",
    "                                   blit=True, interval=interval)\n",
    "    plt.close(fig)\n",
    "    return HTML(anim.to_jshtml())"
   ]
  },
  {
