  {
   "cell_type": "code",
   "execution_count": 113,
   "id": "917db082",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ODE(x,y,i):\n",
    "    '''\n",
    "    Calculates the acceleration of the rocket at the ith time step.\n",
    "    '''\n",
    "    pos_earth, pos_moon = pos_earth_moon(time_step)\n",
    "    d_e = np.sqrt((x - pos_earth[0, i])**2 + (y - pos_earth[1, i])**2)\n",
    "    d_m = np.sqrt((x - pos_moon[0,i])**2 + (y - pos_moon[1,i])**2)\n",
    "\n",
    "    a_rocket = [-G*m_e*(x-pos_earth[0, i])/d_e**3 - G*m_m*(x-pos_moon[0,i])/d_m**3,\n",
    "                -G*m_e*(y-pos_earth[1, i])/d_e**3 - G*m_m*(y-pos_moon[1,i])/d_m**3]\n",
    "\n",
    "    return a_rocket"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 114,
   "id": "cf6f2bf8",
   "metadata": {},
   "outputs": [],
   "source": [
    "#Taylor expansion\n",
    "\n",
    "def taylor_expansion(ODE, time_step,r_rocket):\n",
    "    '''\n",
    "    This function solves a 2nd order ODE using a Taylor expansion with time step: time_step\n",
    "\n",
    "    Inputs:\n",
    "        ODE - function that gives equation of 2nd order ODE\n",
    "        time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    Outputs:\n",
    "        r_rocket - position of the rocket at each time step\n",
    "    '''\n",
    "    # Preallocate arrays for position and velocity\n",
    "    x_r = np.zeros(len(time))\n",
    "    y_r = np.zeros(len(time))\n",
    "    vx_r = np.zeros(len(time))\n",
    "    vy_r = np.zeros(len(time))\n",
    "\n",
    "    # Set Initial conditions\n",
    "    x_r[0] = r_rocket[0]\n",
    "    y_r[0] = r_rocket[1]\n",
    "    vx_r[0] = v_rocket[0]\n",
    "    vy_r[0] = v_rocket[1]\n",
    "\n",
    "    # Calculate acceleration at every point\n",
    "    \n",
    "\n",
    "    for i in range(1, len(time)):\n",
    "        a_rocket = ODE(r_rocket[i], r_rocket[i],i)\n",
    "        x_r[i] = x_r[i-1] + time_step*vx_r[i-1] + 0.5*time_step**2*a_rocket[0]\n",
    "        vx_r[i] = vx_r[i-1] + time_step*a_rocket[1]\n",
    "\n",
    "        y_r[i] = y_r[i-1] + time_step*vy_r[i-1] + 0.5*time_step**2*a_rocket[1]\n",
    "        vy_r[i] = vy_r[i-1] + time_step*a_rocket[1]\n",
    "\n",
    "    r_rocket = [x_r, y_r]\n",
    "\n",
    "    return x_r, y_r"
   ]
  },
