   "id": "b6794764",
   "metadata": {},
   "outputs": [],
   "source": [
    "G = 6.67430e-11 # m^3 kg^-1 s^-2\n",
    "m_e = 5.972e24 # kg\n",
    "m_m = 7.348e22 # kg\n",
    "r = 384400000 # m\n",
    "T = 27.3*24*3600 # s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "4bc3e85f",
   "metadata": {},
   "outputs": [],
   "source": [
    "def pos_earth_moon(time_step,circular=True):\n",
    "    # This function returns the position of the Earth-Moon about it's barycenter\n",
    "\n",
    "    # Inputs \n",
    "    # time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    # Outputs\n",
    "    # pos - Array size (2,2,int(max_time/timestep))\n",
    "    # e.g. pos[0][0] gives the x position of the Earth at each time step\n",
    "    #      pos[0][1] gives the y position of the Earth at each time step\n",
    "    #      pos[1][0] gives the x position of the Moon at each time step\n",
    "    #      pos[1][1] gives the y position of the Moon at each time step\n",
    "\n",
    "    if circular:\n",
    "        T = 2*np.pi*np.sqrt(r**3/(G*(m_e + m_m)))\n",
    "        # Angular velocity\n",
    "        omega = 2*np.pi/T # rad/s\n",
    "\n",
    "        # Time array\n",
    "        max_time = 2*T\n",
    "        time = np.arange(0, max_time, time_step)\n",
    "\n",
    "        # Position arrays\n",
    "        pos = np.zeros((2,2,len(time)))\n",
    "\n",
    "        # Earth position (assuming circular orbit around barycenter)\n",
    "        r_e = r*m_m/(m_e + m_m) # m\n",
    "        pos[0][0] = r_e * np.cos(omega*time) # x position of Earth\n",
    "        pos[0][1] = r_e * np.sin(omega*time) # y position of Earth\n",
    "\n",
    "        # Moon position (assuming circular orbit around barycenter)\n",
    "        r_m = r*m_e/(m_e + m_m) # m\n",
    "        pos[1][0] = -r_m * np.cos(omega*time) # x position of Moon\n",
    "        pos[1][1] = -r_m * np.sin(omega*time) # y position of Moon\n",
    "        return pos\n",
    "    else:\n",
    "        raise(NotImplementedError(\"Elliptical orbits not yet implemented\"))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "6aad88e7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def pos_earth_moon(time_step,circular=True):\n",
    "    # This function returns the position of the Earth-Moon about it's barycenter\n",
    "\n",
    "    # Inputs \n",
    "    # time_step - interval of time in seconds which the position is to be calculated\n",
    "\n",
    "    # Outputs\n",
    "    # pos - Array size (2,2,int(max_time/timestep))\n",
    "    # e.g. pos[0][0] gives the x position of the Earth at each time step\n",
    "    #      pos[0][1] gives the y position of the Earth at each time step\n",
    "    #      pos[1][0] gives the x position of the Moon at each time step\n",
    "    #      pos[1][1] gives the y position of the Moon at each time step\n",
    "\n",
    "    if circular:\n",
    "        T = 2*np.pi*np.sqrt(r**3/(G*(m_e + m_m)))\n",
    "        # Angular velocity\n",
    "        omega = 2*np.pi/T # rad/s\n",
    "\n",
    "        # Time array\n",
    "        max_time = 2*T\n",
    "        time = np.arange(0, max_time, time_step)\n",
    "\n",
    "        # Position arrays\n",
    "        pos = np.zeros((2,2,len(time)))\n",
    "\n",
    "        # Earth position (assuming circular orbit around barycenter)\n",
    "        r_e = r*m_m/(m_e + m_m) # m\n",
    "        x_e = r_e * np.cos(omega*time) # x position of Earth\n",
    "        y_e = r_e * np.sin(omega*time) # y position of Earth\n",
    "\n",
    "        # Moon position (assuming circular orbit around barycenter)\n",
    "        r_m = r*m_e/(m_e + m_m) # m\n",
    "        x_m = -r_m * np.cos(omega*time) # x position of Moon\n",
    "        y_m = -r_m * np.sin(omega*time) # y position of Moon\n",
    "        return x_e, y_e, x_m, y_m\n",
    "    else:\n",
    "        raise(NotImplementedError(\"Elliptical orbits not yet implemented\"))"
   ]
  },
  {
