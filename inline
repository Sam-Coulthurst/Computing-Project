   ],
   "source": [
    "def ODE(x,y):\n",
    "    x_e, y_e, x_m, y_m = pos_earth_moon(time_step)\n",
    "    d_e = np.sqrt((x - x_e)**2 + (y - y_e)**2)\n",
    "    d_m = np.sqrt((x - x_m)**2 + (y - y_m)**2)\n",
    "    return -G*m_e*(x-x_e)/d_e**3 - G*m_m*(x-x_m)/d_m**3"
   ]
  },
